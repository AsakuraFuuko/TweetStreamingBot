{
  "name": "TweetStreamingBot",
  "description": "a bot for telegram",
  "repository": "https://github.com/AsakuraFuuko/TweetStreamingBot",
  "keywords": [
    "bot",
    "telegram",
    "telegram-bot",
    "twitter"
  ],
  "env": {
    "PORT": {
      "description": "listen port",
      "value": "9000"
    },
    "APP_URL": {
      "description": "heroku app url",
      "value": ""
    },
    "TELEGRAM_TOKEN": {
      "description": "telegram bot token",
      "value": ""
    },
    "LOCAL": {
      "description": "local test",
      "value": "false"
    },
    "TWITTER_CONSUMER_KEY": {
      "description": "twitter consumer key",
      "value": ""
    },
    "TWITTER_CONSUMER_SECRET": {
      "description": "twitter consumer secret",
      "value": ""
    },
    "TWITTER_ACCESS_TOKEN_KEY": {
      "description": "twitter access token key",
      "value": ""
    },
    "TWITTER_ACCESS_TOKEN_SECRET": {
      "description": "twitter access token secret",
      "value": ""
    },
    "OWNER_ID": {
      "description": "telegram owner id",
      "value": ""
    },
    "TWITTER_ID": {
      "description": "twitter list form id",
      "value": ""
    },
    "TWITTER_LIST": {
      "description": "twitter list",
      "value": ""
    }
  }
}