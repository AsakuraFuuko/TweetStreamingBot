{
  "name": "TweetStreamingBot",
  "description": "a bot for telegram",
  "repository": "https://github.com/AsakuraFuuko/TweetStreamingBot",
  "keywords": [
    "bot",
    "telegram",
    "telegram-bot",
    "twitter"
  ],
  "env": {
    "PORT": {
      "description": "listen port",
      "value": "9000"
    },
    "APP_URL": {
      "description": "heroku app url",
      "value": ""
    },
    "TELEGRAM_TOKEN": {
      "description": "telegram bot token",
      "value": ""
    },
    "LOCAL": {
      "description": "local test",
      "value": "false"
    },
    "DATABASE_URL": {
      "description": "mongodb url",
      "value": ""
    },
    "TWITTER_CONSUMER_KEY": {
      "description": "twitter consumer key",
      "value": ""
    },
    "TWITTER_CONSUMER_SECRET": {
      "description": "twitter consumer secret",
      "value": ""
    },
    "HEROKU_API_TOKEN": {
      "description": "heroku api token",
      "value": ""
    },
    "HEROKU_APP_NAME": {
      "description": "heroku app name",
      "value": ""
    }
  }
}
